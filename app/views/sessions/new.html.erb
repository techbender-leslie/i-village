<div class="col-md-8 col-md-offset-2">
  <div class="well well-lg">
    <h2><center>You must be logged in to access this neighbor portal<center></h2>
    <br>
    <%= simple_form_for :sessions, { url: sessions_path} do |f| %>
    <%= f.input :email, input_html: { name: "email" } %>
    <%= f.input :passowrd, input_html: { name: "password" } %>

    <%= f.submit "Login".html_safe, class: "btn btn-sm btn-danger"%>
    <% end %>
  </div>
</div>